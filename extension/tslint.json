{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "object-literal-shorthand": false,
    "semicolon": [
      true,
      "always",
      "strict-bound-class-methods"
    ],
    "type-literal-delimiter": true,
    "max-classes-per-file": false,
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "jsx-no-lambda": false
  },
  "rulesDirectory": []
}
